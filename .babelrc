{
    "presets": ["next/babel"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./"],
                "alias": {
                    "components": "src/components",
                    "reducers": "src/reducers",
                    "store": "src/store",
                    "types": "src/types",
                    "utils": "src/utils",
                    "assets": "src/assets"
                },
                "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
        ],
        "@babel/plugin-proposal-nullish-coalescing-operator"
    ]
}
